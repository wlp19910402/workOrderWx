.qm-col{
  position: relative;
}
.qm-col-content{
  padding: 10rpx 0;
  display: flex;
  flex: 1;
  flex-direction: column;
  align-items: center;
  border-left: 1rpx solid rgba(0,0,0,0.1);
  background: #fff;
  z-index: 2;
  position: relative;
  /* background:rgba(36,189,122,0.04) */
}
.qm-col-content:last-child{
  border-left: none;
}
